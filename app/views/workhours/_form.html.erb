<%= simple_form_for(@workhour) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :hours %>
    <%= f.input :note %>
    <%= f.input :date_of_workhour %>
    <%= f.input :user_id %>
    <%= f.input :project_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
